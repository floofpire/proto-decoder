{
  "name": "proto-decoder",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run --hot src/server.ts",
    "generate:mysql": "drizzle-kit generate --dialect=mysql --schema=./src/db/schema",
    "migrate:mesages": "bun run src/scripts/migrateFromFileToDB.ts",
    "find:missing:enrollment": "bun run src/scripts/findMissingEnrollment.ts",
    "clean:patch:notes": "bun run src/scripts/patchNotes.ts",
    "postinstall": "patch-package",
    "prettier": "biome format --write ./src",
    "start": "bun run src/server.ts",
    "preprotots": "pbjs --keep-case -t static-module --no-verify --no-create --no-delimited -o compiled.js csproto/down.proto csproto/up.proto",
    "protots": "pbts -o src/afkprotos.d.ts compiled.js",
    "postprotots": "rm compiled.js && biome format --write --files-max-size 33554432 ./src/afkprotos.d.ts"
  },
  "dependencies": {
    "@biomejs/js-api": "^0.6.2",
    "@biomejs/wasm-nodejs": "^1.8.3",
    "@elysiajs/cron": "^1.0.3",
    "copy-paste": "^1.5.3",
    "crc-32": "^1.2.2",
    "drizzle-orm": "^0.36.0",
    "elysia": "^1.0.27",
    "just-safe-set": "^4.2.1",
    "markdown-table": "^3.0.3",
    "mysql2": "^3.10.2",
    "protobufjs": "^7.2.5",
    "tslog": "^4.9.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/copy-paste": "^1.1.33",
    "bun-types": "latest",
    "drizzle-kit": "^0.25.0",
    "patch-package": "^8.0.0",
    "protobufjs-cli": "^1.1.2"
  },
  "peerDependencies": {
    "typescript": "^5.5.3"
  },
  "trustedDependencies": ["@biomejs/biome"]
}
